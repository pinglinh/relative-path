{
    "name": "relative-path",
    "displayName": "relative_path",
    "description": "You can copy and paste relative path",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyPathRelative"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyPathRelative",
                "title": "Copy relative file / pinglinh"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.copyPathRelative"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "clipboardy": "^1.2.3",
        "relative": "^3.0.2"
    }
}
